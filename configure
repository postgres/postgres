  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  for pgac_format in '%lld' '%qd' '%I64d'; do
if test "$cross_compiling" = yes; then
  pgac_cv_snprintf_long_long_int_format=cross; break
else
  cat >conftest.$ac_ext <<_ACEOF
/* confdefs.h.  */
