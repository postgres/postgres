"    | CREATEROLE | NOCREATEROLE\n"
"    | CREATEUSER | NOCREATEUSER\n"
"    | INHERIT | NOINHERIT\n"
"    | LOGIN | NOLOGIN\n"
"    | CONNECTION LIMIT limite_con\n"
