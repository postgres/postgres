msgstr "  \\do[S+] [МАСКА]        список операторов\n"

#: help.c:249
#, c-format
msgid "  \\dO[S+] [PATTERN]      list collations\n"
msgstr "  \\dO[S+] [МАСКА]        список правил сортировки\n"

#: help.c:250
