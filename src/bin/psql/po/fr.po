#~ "\n"
#~ "et requête_with est:\n"
#~ "\n"
#~ "    nom_requête_with [ ( nom_colonne [, ...] ) ] AS ( select )\n"
#~ "\n"
#~ "TABLE { [ ONLY ] nom_table [ * ] | nom_requête_with }"

