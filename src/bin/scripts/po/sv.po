#, c-format
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Flaggor:\n"

#: createdb.c:221
#, c-format
msgid "  -D, --tablespace=TABLESPACE  default tablespace for the database\n"
msgstr "  -D, --tablespace=TABLESPACE  standardtabellutrymme för databasen\n"

