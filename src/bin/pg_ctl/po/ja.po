msgid " stopped waiting\n"
msgstr " 待機処理が停止されました\n"

#: pg_ctl.c:991
#, c-format
msgid "%s: server did not start in time\n"
msgstr "%s: サーバーは時間内に起動しませんでした\n"

#: pg_ctl.c:997
#, c-format
msgid ""
"%s: could not start server\n"
"Examine the log output.\n"
msgstr ""
"%s: サーバーを起動できませんでした。\n"
"ログ出力を確認してください。\n"

#: pg_ctl.c:1005
msgid "server starting\n"
msgstr "サーバーは起動中です。\n"

