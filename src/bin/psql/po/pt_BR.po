"    [ [ WITH ] \n"
"          [ BINARY ] \n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] 'delimitador' ]\n"
"          [ NULL [ AS ] 'cadeia nula' ] ]\n"
"          [ CSV [ HEADER ]\n"
"                [ QUOTE [ AS ] 'separador' ] \n"
"                [ ESCAPE [ AS ] 'escape' ]\n"
"                [ FORCE NOT NULL coluna [, ...] ]\n"
"\n"
"COPY nome_tabela [ ( coluna [, ...] ) ]\n"
"    TO { 'arquivo' | STDOUT }\n"
"    [ [ WITH ] \n"
"          [ BINARY ]\n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] 'delimitador' ]\n"
"          [ NULL [ AS ] 'cadeia nula' ] ]\n"
"          [ CSV [ HEADER ]\n"
"                [ QUOTE [ AS ] 'separador' ] \n"
"                [ ESCAPE [ AS ] 'escape' ]\n"
"                [ FORCE QUOTE coluna [, ...] ]"

#: sql_help.h:137
msgid "define a new aggregate function"
msgstr "define um nova função de agregação"

