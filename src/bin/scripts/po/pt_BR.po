#, c-format
msgid "%s: clustering of database \"%s\" failed: %s"
msgstr "%s: agrupamento do banco de dados \"%s\" falhou: %s"

#: clusterdb.c:219
#, c-format
msgid "%s: clustering database \"%s\"\n"
msgstr "%s: agrupando banco de dados \"%s\"\n"

