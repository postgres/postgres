#, c-format
msgid "could not open file \"%s\": %m"
msgstr "n'a pas pu ouvrir le fichier « %s » : %m"

#: pg_verifybackup.c:391
#, c-format
msgid "could not stat file \"%s\": %m"
msgstr "n'a pas pu tester le fichier « %s » : %m"

