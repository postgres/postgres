"          [ BINARY ] \n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] 'avdelare' ]\n"
"          [ NULL [ AS ] 'null-sträng' ] ]\n"
"          [ CSV [ HEADER ]\n"
"                [ QUOTE [ AS ] 'citat' ]\n"
"                [ ESCAPE [ AS ] 'escape' ]\n"
"                [ FORCE NOT NULL kolumn [, ...] ]\n"
"\n"
"COPY tabellnamn [ ( kolumn [, ...] ) ]\n"
"    TO { 'filnamn' | STDOUT }\n"
"    [ [ WITH ] \n"
"          [ BINARY ]\n"
"          [ HEADER ]\n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] 'avdelare' ]\n"
"          [ NULL [ AS ] 'null-sträng' ] ]\n"
"          [ CSV [ HEADER ]\n"
"                [ QUOTE [ AS ] 'citat' ]\n"
"                [ ESCAPE [ AS ] 'escape' ]\n"
"                [ FORCE QUOTE kolumn [, ...] ]"

#: sql_help.h:137
