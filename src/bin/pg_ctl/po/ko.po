#, c-format
msgid "could not resolve path \"%s\" to absolute form: %m"
msgstr "\"%s\" 경로를 절대경로로 바꿀 수 없음: %m"

#: ../../common/exec.c:412
#, c-format
msgid "%s() failed: %m"
msgstr "%s() 실패: %m"

#: ../../common/exec.c:550 ../../common/exec.c:595 ../../common/exec.c:687
msgid "out of memory"
msgstr "메모리 부족"

#: ../../common/fe_memutils.c:35 ../../common/fe_memutils.c:75
