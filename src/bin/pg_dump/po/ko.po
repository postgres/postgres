#: pg_dumpall.c:2130 pg_dumpall.c:2156
#, c-format
msgid "%s: executing %s\n"
msgstr "%s: %s 실행중\n"

#: pg_dumpall.c:2136 pg_dumpall.c:2162
#, c-format
