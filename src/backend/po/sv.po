#, c-format
msgid "  -d 1-5             debugging level\n"
msgstr "  -d 1-5             debug-nivå\n"

#: main/main.c:331
#, c-format
msgid "  -D DATADIR         database directory\n"
msgstr "  -D DATADIR         databaskatalog\n"

