#: pg_ctl.c:1207
#, c-format
msgid "%s: cannot promote server; server is not in standby mode\n"
msgstr ""
"%s: no se puede promover el servidor;\n"
"el servidor no está en modo «standby»\n"

#: pg_ctl.c:1222
#, c-format
msgid "%s: could not create promote signal file \"%s\": %s\n"
