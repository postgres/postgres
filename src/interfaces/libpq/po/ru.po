#, c-format
msgid "channel binding required, but SSL not in use"
msgstr "требуется привязка каналов, но SSL не используется"

#: fe-auth.c:443
#, c-format
msgid "duplicate SASL authentication request"
msgstr "повторный запрос аутентификации SASL"

