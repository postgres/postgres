msgid ""
"\n"
"Environment variables:\n"
msgstr ""
"\n"
"Omgivningsvariabler:\n"

#: help.c:519
msgid ""
"  NAME=VALUE [NAME=VALUE] psql ...\n"
"  or \\setenv NAME [VALUE] inside psql\n"
"\n"
msgstr ""
"  NAMN=VÄRDE [NAMN=VÄRDE] psql ...\n"
"  eller \\setenv NAMN [VÄRDE] inne psql\n"
"\n"

