"    { SET DEFAULT ausdruck | DROP DEFAULT }\n"
"ALTER DOMAIN name\n"
"    { SET | DROP } NOT NULL\n"
"ALTER DOMAIN name\n"
"    ADD domänen_constraint\n"
"ALTER DOMAIN name\n"
"    DROP CONSTRAINT constraint_name [ RESTRICT | CASCADE ]\n"
"ALTER DOMAIN name\n"
"    OWNER TO neuer_eigentümer"

