    <sect4>
     <title>Monitoring</title>

     <itemizedlist>

      <listitem>
       <!--
       Author: Peter Eisentraut <peter_e@gmx.net>
       2016-11-18 [67dc4ccbb] Add pg_sequences view
       -->
