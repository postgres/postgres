#: sql_help.h:522 sql_help.h:558 sql_help.h:582
msgid ""
"[ WITH [ RECURSIVE ] with_query [, ...] ]\n"
"SELECT [ ALL | DISTINCT [ ON ( expression [, ...] ) ] ]\n"
"    * | expression [ [ AS ] output_name ] [, ...]\n"
"    [ FROM from_item [, ...] ]\n"
"    [ WHERE condition ]\n"
"    [ GROUP BY expression [, ...] ]\n"
"    [ HAVING condition [, ...] ]\n"
"    [ WINDOW window_name AS ( window_definition ) [, ...] ]\n"
"    [ { UNION | INTERSECT | EXCEPT } [ ALL ] select ]\n"
"    [ ORDER BY expression [ ASC | DESC | USING operator ] [ NULLS { FIRST | "
"LAST } ] [, ...] ]\n"
"    [ LIMIT { count | ALL } ]\n"
"    [ OFFSET start [ ROW | ROWS ] ]\n"
"    [ FETCH { FIRST | NEXT } [ count ] { ROW | ROWS } ONLY ]\n"
"    [ FOR { UPDATE | SHARE } [ OF table_name [, ...] ] [ NOWAIT ] [...] ]\n"
"\n"
"where from_item can be one of:\n"
"\n"
"    [ ONLY ] table_name [ * ] [ [ AS ] alias [ ( column_alias [, ...] ) ] ]\n"
"    ( select ) [ AS ] alias [ ( column_alias [, ...] ) ]\n"
"    with_query_name [ [ AS ] alias [ ( column_alias [, ...] ) ] ]\n"
"    function_name ( [ argument [, ...] ] ) [ AS ] alias [ ( column_alias "
"[, ...] | column_definition [, ...] ) ]\n"
"    function_name ( [ argument [, ...] ] ) AS ( column_definition [, ...] )\n"
"    from_item [ NATURAL ] join_type from_item [ ON join_condition | USING "
"( join_column [, ...] ) ]\n"
"\n"
"and with_query is:\n"
"\n"
"    with_query_name [ ( column_name [, ...] ) ] AS ( select )\n"
"\n"
"TABLE { [ ONLY ] table_name [ * ] | with_query_name }"
msgstr ""
"[ WITH [ RECURSIVE ] with_query [, ...]]\n"
"SELECT [ ALL | DISTINCT [ ON ( 運算式 [, ...]) ] ]\n"
"    * | 運算式 [ [ AS ] output_name ] [, ...]\n"
"[ FROM 來源項目 [, ...]]\n"
"    [ WHERE 條件 ]\n"
"    [ GROUP BY 運算式 [, ...]]\n"
"    [ HAVING 條件 [, ...]]\n"
"    [ WINDOW window_name AS ( window_definition ) [, ...]]\n"
"    [ { UNION | INTERSECT | EXCEPT } [ ALL ] 選取 ]\n"
"    [ ORDER BY 運算式 [ ASC | DESC | USING 運算子 ] [ NULLS { FIRST | "
"LAST } ] [, ...]]\n"
"    [ LIMIT { 計數 | ALL } ]\n"
"    [ OFFSET 開始 [ ROW | ROWS ] ]\n"
"    [ FETCH { FIRST | NEXT } [ 計數 ] { ROW | ROWS } ONLY ]\n"
"    [ FOR { UPDATE | SHARE } [ OF table_name [, ...]] [ NOWAIT ] [...]]\n"
"\n"
"其中 from_item 可以是下列其中一項:\n"
"\n"
"[ ONLY ] table_name [ * ] [ [ AS ] 別名 [ ( column_alias [, ...]) ] ]\n"
"    ( 選取 ) [ AS ] 別名 [ ( column_alias [, ...]) ]\n"
"    with_query_name [ [ AS ] 別名 [ ( column_alias [, ...]) ] ]\n"
"    function_name ( [ 參數 [, ...]] ) [ AS ] 別名 [ ( column_alias [, ...]| "
"column_definition [, ...]) ]\n"
"    function_name ( [ 參數 [, ...]] ) AS ( column_definition [, ...])\n"
"    from_item [ NATURAL ] join_type from_item [ ON join_condition | USING "
"( join_column [, ...]) ]\n"
"\n"
"且 with_query 是:\n"
"\n"
"    with_query_name [ ( column_name [, ...]) ] AS ( 選取 )\n"
"\n"
"TABLE { [ ONLY ] table_name [ * ] | with_query_name }"

#: sql_help.h:526
msgid ""
"[ WITH [ RECURSIVE ] with_query [, ...] ]\n"
"SELECT [ ALL | DISTINCT [ ON ( expression [, ...] ) ] ]\n"
"    * | expression [ [ AS ] output_name ] [, ...]\n"
"    INTO [ TEMPORARY | TEMP ] [ TABLE ] new_table\n"
"    [ FROM from_item [, ...] ]\n"
"    [ WHERE condition ]\n"
"    [ GROUP BY expression [, ...] ]\n"
"    [ HAVING condition [, ...] ]\n"
