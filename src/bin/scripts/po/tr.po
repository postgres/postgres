#: reindexdb.c:244
#, c-format
msgid "%s: reindexing of database \"%s\" failed: %s"
msgstr "%s: \"%s\" veritabanının yeniden indekslenmesi başarısız oldu: %s"

#: reindexdb.c:273
#, c-format
msgid "%s: reindexing database \"%s\"\n"
msgstr "%s: \"%s\" veritabanı yeniden indeksleniyor\n"

