msgstr "確認目前的事物交易"

# sql_help.h:122
#: sql_help.h:162
msgid "COMMIT [ WORK | TRANSACTION ]"
msgstr "COMMIT [ WORK | TRANSACTION ]"

#: sql_help.h:165
msgid "commit a transaction that was earlier prepared for two-phase commit"
msgstr "提交一項事務交易這是兩階段提交的先前準備"

#: sql_help.h:166
msgid "COMMIT PREPARED transaction_id"
msgstr "COMMIT PREPARED 事務交易_編號"

# sql_help.h:125
#: sql_help.h:169
msgid "copy data between a file and a table"
msgstr "在檔案和資料表間複製資料"

#: sql_help.h:170
msgid ""
"COPY tablename [ ( column [, ...] ) ]\n"
"    FROM { 'filename' | STDIN }\n"
"    [ [ WITH ] \n"
"          [ BINARY ]\n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] 'delimiter' ]\n"
"          [ NULL [ AS ] 'null string' ]\n"
"          [ CSV [ HEADER ]\n"
"                [ QUOTE [ AS ] 'quote' ] \n"
"                [ ESCAPE [ AS ] 'escape' ]\n"
"                [ FORCE NOT NULL column [, ...] ]\n"
"\n"
"COPY { tablename [ ( column [, ...] ) ] | ( query ) }\n"
"    TO { 'filename' | STDOUT }\n"
"    [ [ WITH ] \n"
"          [ BINARY ]\n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] 'delimiter' ]\n"
"          [ NULL [ AS ] 'null string' ]\n"
"          [ CSV [ HEADER ]\n"
"                [ QUOTE [ AS ] 'quote' ] \n"
"                [ ESCAPE [ AS ] 'escape' ]\n"
"                [ FORCE QUOTE column [, ...] ]"
msgstr ""
"COPY 資料表名稱 [ ( 資料行 [, ...]) ]\n"
"    FROM { &apos;檔案名稱&apos; | STDIN }\n"
"    [ [ WITH ] \n"
"          [ BINARY ]\n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] &apos;分隔符號&apos; ]\n"
"          [ NULL [ AS ] &apos;null 字串&apos; ]\n"
"          [ CSV [ HEADER ]\n"
"                [ QUOTE [ AS ] &apos;引號&apos; ] \n"
"                [ ESCAPE [ AS ] &apos;逸出字元&apos; ]\n"
"                [ FORCE NOT NULL 資料行 [, ...]]\n"
"\n"
"COPY { 資料表名稱 [ ( 資料行 [, ...]) ] | ( 查詢 ) }\n"
"    TO { &apos;檔案名稱&apos; | STDOUT }\n"
"    [ [ WITH ] \n"
"          [ BINARY ]\n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] &apos;分隔符號&apos; ]\n"
"          [ NULL [ AS ] &apos;null 字串&apos; ]\n"
"          [ CSV [ HEADER ]\n"
"                [ QUOTE [ AS ] &apos;引號&apos; ] \n"
"                [ ESCAPE [ AS ] &apos;逸出字元&apos; ]\n"
"                [ FORCE QUOTE 資料行 [, ...]]"

# sql_help.h:129
#: sql_help.h:173
msgid "define a new aggregate function"
msgstr "定義一個新的聚集函數"

#: sql_help.h:174
msgid ""
"CREATE AGGREGATE name ( input_data_type [ , ... ] ) (\n"
"    SFUNC = sfunc,\n"
"    STYPE = state_data_type\n"
"    [ , FINALFUNC = ffunc ]\n"
"    [ , INITCOND = initial_condition ]\n"
"    [ , SORTOP = sort_operator ]\n"
")\n"
"\n"
"or the old syntax\n"
"\n"
"CREATE AGGREGATE name (\n"
"    BASETYPE = base_type,\n"
"    SFUNC = sfunc,\n"
"    STYPE = state_data_type\n"
"    [ , FINALFUNC = ffunc ]\n"
"    [ , INITCOND = initial_condition ]\n"
"    [ , SORTOP = sort_operator ]\n"
")"
msgstr ""
"CREATE AGGREGATE 名稱 ( input_data_type [ , ...] ) (\n"
"    SFUNC = sfunc,\n"
"    STYPE = state_data_type\n"
"    [ , FINALFUNC = ffunc ]\n"
"    [ , INITCOND = initial_condition ]\n"
"    [ , SORTOP = sort_operator ]\n"
")\n"
"\n"
"或舊語法\n"
"\n"
"CREATE AGGREGATE 名稱 (\n"
"    BASETYPE = base_type,\n"
"    SFUNC = sfunc,\n"
"    STYPE = state_data_type\n"
"    [ , FINALFUNC = ffunc ]\n"
"    [ , INITCOND = initial_condition ]\n"
"    [ , SORTOP = sort_operator ]\n"
")"

# sql_help.h:133
#: sql_help.h:177
msgid "define a new cast"
msgstr "建立新的型別轉換"

#: sql_help.h:178
msgid ""
"CREATE CAST (sourcetype AS targettype)\n"
"    WITH FUNCTION funcname (argtypes)\n"
"    [ AS ASSIGNMENT | AS IMPLICIT ]\n"
"\n"
"CREATE CAST (sourcetype AS targettype)\n"
"    WITHOUT FUNCTION\n"
"    [ AS ASSIGNMENT | AS IMPLICIT ]\n"
"\n"
"CREATE CAST (sourcetype AS targettype)\n"
"    WITH INOUT\n"
"    [ AS ASSIGNMENT | AS IMPLICIT ]"
msgstr ""
"CREATE CAST (來源類型 AS 目標類型)\n"
"    WITH FUNCTION funcname (argtypes)\n"
"    [ AS ASSIGNMENT | AS IMPLICIT ]\n"
"\n"
"CREATE CAST (來源類型 AS 目標類型)\n"
"    WITHOUT FUNCTION\n"
"    [ AS ASSIGNMENT | AS IMPLICIT ]\n"
"\n"
"CREATE CAST (來源類型 AS 目標類型)\n"
"    WITH INOUT\n"
"    [ AS ASSIGNMENT | AS IMPLICIT ]"

# sql_help.h:137
#: sql_help.h:181
msgid "define a new constraint trigger"
msgstr "定義一個新的約束限制觸發器"

#: sql_help.h:182
msgid ""
"CREATE CONSTRAINT TRIGGER name\n"
"    AFTER event [ OR ... ]\n"
"    ON table_name\n"
"    [ FROM referenced_table_name ]\n"
"    { NOT DEFERRABLE | [ DEFERRABLE ] { INITIALLY IMMEDIATE | INITIALLY "
"DEFERRED } }\n"
"    FOR EACH ROW\n"
"    EXECUTE PROCEDURE funcname ( arguments )"
msgstr ""
"CREATE CONSTRAINT TRIGGER 名稱\n"
"    AFTER 事件 [ OR ...]\n"
"    ON table_name\n"
"    [ FROM referenced_table_name ]\n"
"    { NOT DEFERRABLE | [ DEFERRABLE ] { INITIALLY IMMEDIATE | INITIALLY "
"DEFERRED } }\n"
"    FOR EACH ROW\n"
"    EXECUTE PROCEDURE funcname ( 參數 )"

# sql_help.h:141
#: sql_help.h:185
msgid "define a new encoding conversion"
msgstr "定義一個新的字元編碼轉換"

#: sql_help.h:186
msgid ""
"CREATE [ DEFAULT ] CONVERSION name\n"
"    FOR source_encoding TO dest_encoding FROM funcname"
msgstr ""
"CREATE [ DEFAULT ] CONVERSION 名稱\n"
"    FOR source_encoding TO dest_encoding FROM funcname"

# sql_help.h:145
#: sql_help.h:189
msgid "create a new database"
msgstr "建立新的資料庫"

#: sql_help.h:190
msgid ""
"CREATE DATABASE name\n"
"    [ [ WITH ] [ OWNER [=] dbowner ]\n"
"           [ TEMPLATE [=] template ]\n"
"           [ ENCODING [=] encoding ]\n"
"           [ LC_COLLATE [=] lc_collate ]\n"
"           [ LC_CTYPE [=] lc_ctype ]\n"
"           [ TABLESPACE [=] tablespace ]\n"
"           [ CONNECTION LIMIT [=] connlimit ] ]"
msgstr ""
"CREATE DATABASE 名稱\n"
"    [ [ WITH ] [ OWNER [=] 資料庫擁有者 ]\n"
"           [ TEMPLATE [=] 樣板 ]\n"
"           [ ENCODING [=] 編碼 ]\n"
"           [ LC_COLLATE [=] lc_collate ]\n"
"           [ LC_CTYPE [=] lc_ctype ]\n"
"           [ TABLESPACE [=] 資料表空間 ]\n"
"           [ CONNECTION LIMIT [=] connlimit ] ]"

# sql_help.h:149
#: sql_help.h:193
msgid "define a new domain"
msgstr "建立新的共同值域"

#: sql_help.h:194
msgid ""
"CREATE DOMAIN name [ AS ] data_type\n"
"    [ DEFAULT expression ]\n"
"    [ constraint [ ... ] ]\n"
"\n"
"where constraint is:\n"
"\n"
"[ CONSTRAINT constraint_name ]\n"
"{ NOT NULL | NULL | CHECK (expression) }"
msgstr ""
"CREATE DOMAIN 名稱 [ AS ] data_type\n"
"    [ DEFAULT 運算式 ]\n"
"    [ 限制 [ ...] ]\n"
"\n"
"其中「運算式」是:\n"
"\n"
"[ CONSTRAINT constraint_name ]\n"
"{ NOT NULL | NULL | CHECK (運算式) }"

# sql_help.h:205
#: sql_help.h:197
msgid "define a new foreign-data wrapper"
msgstr "定義新的外部資料包裝函式"

#: sql_help.h:198
msgid ""
"CREATE FOREIGN DATA WRAPPER name\n"
"    [ VALIDATOR valfunction | NO VALIDATOR ]\n"
"    [ OPTIONS ( option 'value' [, ... ] ) ]"
msgstr ""
"CREATE FOREIGN DATA WRAPPER 名稱\n"
"    [ VALIDATOR valfunction | NO VALIDATOR ]\n"
"    [ OPTIONS ( 選項 &apos;值&apos; [, ...] ) ]"

# sql_help.h:153
#: sql_help.h:201
msgid "define a new function"
msgstr "建立新的函數"

#: sql_help.h:202
msgid ""
"CREATE [ OR REPLACE ] FUNCTION\n"
"    name ( [ [ argmode ] [ argname ] argtype [ { DEFAULT | = } defexpr ] "
"[, ...] ] )\n"
"    [ RETURNS rettype\n"
"      | RETURNS TABLE ( colname coltype [, ...] ) ]\n"
"  { LANGUAGE langname\n"
"    | WINDOW\n"
"    | IMMUTABLE | STABLE | VOLATILE\n"
"    | CALLED ON NULL INPUT | RETURNS NULL ON NULL INPUT | STRICT\n"
"    | [ EXTERNAL ] SECURITY INVOKER | [ EXTERNAL ] SECURITY DEFINER\n"
"    | COST execution_cost\n"
"    | ROWS result_rows\n"
"    | SET configuration_parameter { TO value | = value | FROM CURRENT }\n"
"    | AS 'definition'\n"
"    | AS 'obj_file', 'link_symbol'\n"
"  } ...\n"
"    [ WITH ( attribute [, ...] ) ]"
msgstr ""
"CREATE [ OR REPLACE ] FUNCTION\n"
"    名稱 ( [ [ argmode ] [ argname ] argtype [ { DEFAULT | = } defexpr ] "
"[, ...]] )\n"
"    [ RETURNS rettype\n"
"      | RETURNS TABLE ( colname coltype [, ...]) ]\n"
"  { LANGUAGE 語言名稱\n"
"    | WINDOW\n"
"    | IMMUTABLE | STABLE | VOLATILE\n"
"    | CALLED ON NULL INPUT | RETURNS NULL ON NULL INPUT | STRICT\n"
"    | [ EXTERNAL ] SECURITY INVOKER | [ EXTERNAL ] SECURITY DEFINER\n"
"    | COST execution_cost\n"
"    | ROWS result_rows\n"
"    | SET configuration_parameter { TO value | = value | FROM CURRENT }\n"
"    | AS &apos;定義&apos;\n"
"    | AS &apos;obj_file&apos;, &apos;link_symbol&apos;\n"
"  } ...\n"
"[ WITH ( 屬性 [, ...]) ]"

# sql_help.h:189
#: sql_help.h:205 sql_help.h:229 sql_help.h:285
msgid "define a new database role"
msgstr "定義一個新資料庫角色"

#: sql_help.h:206
msgid ""
"CREATE GROUP name [ [ WITH ] option [ ... ] ]\n"
"\n"
"where option can be:\n"
"    \n"
"      SUPERUSER | NOSUPERUSER\n"
"    | CREATEDB | NOCREATEDB\n"
"    | CREATEROLE | NOCREATEROLE\n"
"    | CREATEUSER | NOCREATEUSER\n"
"    | INHERIT | NOINHERIT\n"
"    | LOGIN | NOLOGIN\n"
"    | [ ENCRYPTED | UNENCRYPTED ] PASSWORD 'password'\n"
"    | VALID UNTIL 'timestamp' \n"
"    | IN ROLE rolename [, ...]\n"
"    | IN GROUP rolename [, ...]\n"
"    | ROLE rolename [, ...]\n"
"    | ADMIN rolename [, ...]\n"
"    | USER rolename [, ...]\n"
"    | SYSID uid"
msgstr ""
"CREATE GROUP 名稱 [ [ WITH ] 選項 [ ...] ]\n"
"\n"
"其中「選項」可以是:\n"
"\n"
"      SUPERUSER | NOSUPERUSER\n"
"    | CREATEDB | NOCREATEDB\n"
"    | CREATEROLE | NOCREATEROLE\n"
"    | CREATEUSER | NOCREATEUSER\n"
"    | INHERIT | NOINHERIT\n"
"    | LOGIN | NOLOGIN\n"
"    | [ ENCRYPTED | UNENCRYPTED ] PASSWORD &apos;密碼&apos;\n"
"    | VALID UNTIL &apos;時標&apos; \n"
"    | IN ROLE 角色名稱 [, ...]\n"
"| IN GROUP 角色名稱 [, ...]\n"
"| ROLE 角色名稱 [, ...]\n"
"| ADMIN 角色名稱 [, ...]\n"
"| USER 角色名稱 [, ...]\n"
"| SYSID uid"

# sql_help.h:161
#: sql_help.h:209
msgid "define a new index"
msgstr "建立新的索引"

#: sql_help.h:210
msgid ""
"CREATE [ UNIQUE ] INDEX [ CONCURRENTLY ] name ON table [ USING method ]\n"
"    ( { column | ( expression ) } [ opclass ] [ ASC | DESC ] [ NULLS { FIRST "
"| LAST } ] [, ...] )\n"
"    [ WITH ( storage_parameter = value [, ... ] ) ]\n"
"    [ TABLESPACE tablespace ]\n"
"    [ WHERE predicate ]"
msgstr ""
"CREATE [ UNIQUE ] INDEX [ CONCURRENTLY ] 名稱 ON 資料表 [ USING 方法 ]\n"
"    ( { 資料行 | ( 運算式 ) } [ 運算子類別 ] [ ASC | DESC ] [ NULLS { FIRST "
"| LAST } ] [, ...])\n"
"    [ WITH ( storage_parameter = 值 [, ...] ) ]\n"
"    [ TABLESPACE 資料表空間 ]\n"
"    [ WHERE 述詞 ]"

# sql_help.h:165
#: sql_help.h:213
msgid "define a new procedural language"
msgstr "建立新的程序語言"

#: sql_help.h:214
msgid ""
"CREATE [ PROCEDURAL ] LANGUAGE name\n"
"CREATE [ TRUSTED ] [ PROCEDURAL ] LANGUAGE name\n"
"    HANDLER call_handler [ VALIDATOR valfunction ]"
msgstr ""
"CREATE [ PROCEDURAL ] LANGUAGE 名稱\n"
"CREATE [ TRUSTED ] [ PROCEDURAL ] LANGUAGE 名稱\n"
"    HANDLER call_handler [ VALIDATOR valfunction ]"

# sql_help.h:173
#: sql_help.h:217
msgid "define a new operator"
msgstr "建立新的運算子"

#: sql_help.h:218
msgid ""
"CREATE OPERATOR name (\n"
"    PROCEDURE = funcname\n"
"    [, LEFTARG = lefttype ] [, RIGHTARG = righttype ]\n"
"    [, COMMUTATOR = com_op ] [, NEGATOR = neg_op ]\n"
"    [, RESTRICT = res_proc ] [, JOIN = join_proc ]\n"
"    [, HASHES ] [, MERGES ]\n"
")"
msgstr ""
"CREATE OPERATOR 名稱 (\n"
"    PROCEDURE = funcname\n"
"    [, LEFTARG = lefttype ] [, RIGHTARG = righttype ]\n"
"    [, COMMUTATOR = com_op ] [, NEGATOR = neg_op ]\n"
"    [, RESTRICT = res_proc ] [, JOIN = join_proc ]\n"
