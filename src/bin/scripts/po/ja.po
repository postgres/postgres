#, c-format
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"オプション:\n"

#: clusterdb.c:273
#, c-format
msgid "  -a, --all                 cluster all databases\n"
msgstr "  -a, --all                 すべてのデータベースをクラスタ化する\n"

#: clusterdb.c:274
#, c-format
msgid "  -d, --dbname=DBNAME       database to cluster\n"
msgstr "  -d, --dbname=データベース名  クラスタ化するデータベース\n"

