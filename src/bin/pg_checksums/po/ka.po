#, c-format
msgid "error: "
msgstr "შეცდომა: "

#: ../../../src/common/logging.c:283
#, c-format
msgid "warning: "
msgstr "გაფრთხილება: "

#: ../../../src/common/logging.c:294
#, c-format
msgid "detail: "
msgstr "დეტალები: "

#: ../../../src/common/logging.c:301
#, c-format
