msgid "  -F c|t                   specify backup file format\n"
msgstr "  -F c|f                   Format der Backup-Datei\n"

#: pg_restore.c:392
msgid ""
"  -i                       proceed even when server version mismatches\n"
msgstr ""
"  -i                       trotz unpassender Serverversion fortsetzen\n"

#: pg_restore.c:393
