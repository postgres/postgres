msgstr "kopiert Daten zwischen einer Datei und einer Tabelle"

#: sql_help.h:126
msgid ""
"COPY tablename [ ( column [, ...] ) ]\n"
"    FROM { 'filename' | STDIN }\n"
"    [ [ WITH ] \n"
"          [ BINARY ] \n"
"          [ OIDS ]\n"
"          [ DELIMITER [ AS ] 'delimiter' ]\n"
"          [ NULL [ AS ] 'null string' ]\n"
