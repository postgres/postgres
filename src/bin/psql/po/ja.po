#: help.c:455
#, c-format
msgid ""
"  pager\n"
"    control when an external pager is used [yes, no, always]\n"
msgstr ""
"  pager\n"
"    いつ外部ページャーを使うかを制御 [yes, no, always]\n"

#: help.c:457
#, c-format
msgid ""
"  recordsep\n"
"    record (line) separator for unaligned output\n"
msgstr ""
"  recordsep\n"
