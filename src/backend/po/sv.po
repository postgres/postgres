#: main/main.c:326
#, c-format
msgid "Options:\n"
msgstr "Flaggor:\n"

#: main/main.c:327
#, c-format
msgid "  -B NBUFFERS        number of shared buffers\n"
msgstr "  -B NBUFFERS        antalet delade buffertar\n"

