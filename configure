_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  pgac_cv_prog_CC_cflags__Wdeclaration_after_statement=yes
else
  pgac_cv_prog_CC_cflags__Wdeclaration_after_statement=no
