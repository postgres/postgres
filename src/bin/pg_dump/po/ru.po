#, c-format
msgid "  -d, --dbname=DBNAME      database to dump\n"
msgstr "  -d, --dbname=БД          имя базы данных для выгрузки\n"

#: pg_dump.c:1053 pg_dumpall.c:663 pg_restore.c:511
#, c-format
