msgstr "  --rows-per-insert=NROWS      INSERT毎の行数; --insertsを暗黙的に指定する\n"

#: pg_dump.c:1048
#, c-format
msgid "  --section=SECTION            dump named section (pre-data, data, or post-data)\n"
msgstr ""
"  --section=SECTION            指定したセクション(pre-data、data または\n"
"                               post-data)をダンプする\n"

