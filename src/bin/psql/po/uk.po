"  \\lo_import FILE [COMMENT]\n"
"  \\lo_list\n"
"  \\lo_unlink LOBOID      large object operations\n"
msgstr "  \\lo_export LOBOID FILE\n"
"  \\lo_import FILE [COMMENT]\n"
"  \\lo_list\n"
"  \\lo_unlink LOBOID      операції з великими об'єктами\n"

