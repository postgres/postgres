  withval=$with_segsize;
  case $withval in
    yes)
      { { $as_echo "$as_me:$LINENO: error: argument required for --with-segsize option" >&5
$as_echo "$as_me: error: argument required for --with-segsize option" >&2;}
   { (exit 1); exit 1; }; }
      ;;
    no)
      { { $as_echo "$as_me:$LINENO: error: argument required for --with-segsize option" >&5
$as_echo "$as_me: error: argument required for --with-segsize option" >&2;}
