msgid "Usage:\n"
msgstr "Использование:\n"

#: createdb.c:219
#, c-format
msgid "  %s [OPTION]... [DBNAME] [DESCRIPTION]\n"
msgstr "  %s [ОПЦИЯ]... [DBNAME] [DESCRIPTION]\n"

#: clusterdb.c:236
