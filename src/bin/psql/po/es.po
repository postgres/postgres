#: help.c:467
msgid ""
"  VERSION\n"
"  VERSION_NAME\n"
"  VERSION_NUM\n"
"    psql's version (in verbose string, short string, or numeric format)\n"
msgstr ""
"  VERSION\n"
"  VERSION_NAME\n"
"  VERSION_NUM\n"
