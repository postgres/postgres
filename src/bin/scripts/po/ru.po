msgstr "  %s [OPTION]... LANGNAME [DBNAME]\n"

#: createlang.c:302
msgid "  -d, --dbname=DBNAME       database to install language in\n"
msgstr "  -d, --dbname=DBNAME      название базы данных,  в которую устанавливается поддержка языка\n"

#: clusterdb.c:240
