   { (exit 1); exit 1; }; }
      ;;
    no)
      { { echo "$as_me:$LINENO: error: argument required for --with-blocksize option" >&5
echo "$as_me: error: argument required for --with-blocksize option" >&2;}
   { (exit 1); exit 1; }; }
      ;;
