      <entry>Description</entry>
     </row>
    </thead>

    <tbody>
     <row>
      <entry><literal>role_name</literal></entry>
      <entry><type>sql_identifier</type></entry>
      <entry>Name of a role</entry>
     </row>
    </tbody>
   </tgroup>
  </table>
 </sect1>

