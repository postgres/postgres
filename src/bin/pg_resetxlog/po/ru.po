#: pg_resetxlog.c:140
#, c-format
msgid "%s: invalid argument for option -e\n"
msgstr "%s: недопустимый аргумент параметра -e\n"

#: pg_resetxlog.c:141 pg_resetxlog.c:156 pg_resetxlog.c:171 pg_resetxlog.c:186
