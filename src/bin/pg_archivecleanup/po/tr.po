#~ msgid "%s: removing file \"%s\"\n"
#~ msgstr "%s: \"%s\" dosyası kaldırılıyor\n"

#~ msgid "%s: file \"%s\" would be removed\n"
#~ msgstr "%s: \"%s\" dosyası kaldırılacak\n"
