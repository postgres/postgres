DROP TABLE NAME_TBL;
DO $$
DECLARE r text[];
BEGIN
  r := parse_ident('Schemax.Tabley');
  RAISE NOTICE '%', format('%I.%I', r[1], r[2]);
  r := parse_ident('"SchemaX"."TableY"');
  RAISE NOTICE '%', format('%I.%I', r[1], r[2]);
END;
$$;
NOTICE:  schemax.tabley
NOTICE:  "SchemaX"."TableY"
SELECT parse_ident('foo.boo');
