msgstr "  -A, --no-align           使用非對齊資料表輸出模式\n"

# help.c:119
#: help.c:120
#, c-format
msgid ""
"  -F, --field-separator=STRING\n"
"                           set field separator (default: \"%s\")\n"
msgstr ""
"  -F, --field-separator=STRING\n"
"                           設定欄位分隔符號 (預設:\"%s\")\n"

# help.c:114
#: help.c:123
#, c-format
msgid "  -H, --html               HTML table output mode\n"
