#, c-format
msgid ""
"\n"
"General options:\n"
msgstr ""
"\n"
"Общие параметры:\n"

#: pg_dump.c:991
#, c-format
msgid "  -f, --file=FILENAME          output file or directory name\n"
msgstr "  -f, --file=ИМЯ               имя выходного файла или каталога\n"

