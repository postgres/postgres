msgstr ""
"  IGNOREEOF          количество EOF для завершения интерактивного сеанса\n"

#: help.c:363
#, c-format
msgid "  LASTOID            value of the last affected OID\n"
msgstr "  LASTOID            значение последнего задействованного OID\n"

