#: ../../../src/common/logging.c:236
#, c-format
msgid "fatal: "
msgstr "fatalt: "

#: ../../../src/common/logging.c:243
#, c-format
msgid "error: "
msgstr "fel: "

#: ../../../src/common/logging.c:250
