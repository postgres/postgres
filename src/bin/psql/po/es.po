msgstr "   (opciones: S = mostrar objs. de sistema, x = modo expandido, + = más detalle)\n"

#: help.c:222
msgid "  \\d[Sx+]                list tables, views, and sequences\n"
msgstr "  \\d[Sx+]            listar tablas, vistas y secuencias\n"

#: help.c:223
