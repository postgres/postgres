#: option.c:280
#, c-format
msgid "Options:\n"
msgstr "Options :\n"

#: option.c:281
#, c-format
msgid "  -b, --old-bindir=BINDIR       old cluster executable directory\n"
msgstr "  -b, --old-bindir=DIRBIN       répertoire des exécutables de l'ancienne instance\n"

