#: descriptor.c:64
#, c-format
msgid "variable \"%s\" must have a numeric type"
msgstr "la variable «%s» debe tener tipo numérico"

#: descriptor.c:124 descriptor.c:146
#, c-format
msgid "descriptor \"%s\" does not exist"
msgstr "no existe el descriptor «%s»"

