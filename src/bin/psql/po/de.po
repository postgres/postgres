"\n"
"ALTER DATABASE name RENAME TO neuer_name\n"
"\n"
"ALTER DATABASE name OWNER TO neuer_eigentümer\n"
"\n"
"ALTER DATABASE name SET TABLESPACE neuer_tablespace\n"
