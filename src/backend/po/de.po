#, c-format
msgid "  --help          show this help, then exit\n"
msgstr "  --help          diese Hilfe anzeigen, dann beenden\n"

#: main/main.c:294
#, c-format
msgid "  --version       output version information, then exit\n"
msgstr "  --version       Versionsinformationen anzeigen, dann beenden\n"

